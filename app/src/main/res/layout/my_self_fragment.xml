<?xml version="1.0" encoding="utf-8"?>
<!-- 第四个子页面 -->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gxian"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_80px_in720p"
        android:background="@color/home_pager_title"
        android:padding="@dimen/_15px_in720p" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="@string/my_title"
            android:textSize="@dimen/_36px_in720p" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/user"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_140px_in720p"
        android:background="@color/white" >

        <ImageView
            android:id="@+id/user_head"
            android:layout_width="@dimen/_88px_in720p"
            android:layout_height="@dimen/_88px_in720p"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/_20px_in720p"
            android:background="@drawable/user_head" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/_20px_in720p"
            android:layout_toRightOf="@id/user_head"
            android:gravity="center_vertical"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/user_name_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_5px_in720p"
                android:layout_marginLeft="@dimen/_10px_in720p"
                android:text="索博"
                android:textColor="@color/texthei"
                android:textSize="@dimen/_30px_in720p" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_5px_in720p"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/user_phone_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="@dimen/_10px_in720p"
                    android:text="15787856897"
                    android:textColor="#333333"
                    android:textSize="@dimen/_27px_in720p" />
            </LinearLayout>
        </LinearLayout>

        <ImageView
            android:layout_width="@dimen/_20px_in720p"
            android:layout_height="@dimen/_30px_in720p"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/_20px_in720p"
            android:background="@drawable/back_xym" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginLeft="@dimen/_20px_in720p"
        android:layout_marginRight="@dimen/_20px_in720p"
        android:background="@color/gxian" />

    <RelativeLayout
        android:id="@+id/user_jifen_lay"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_100px_in720p"
        android:background="@color/white" >

        <TextView
            android:id="@+id/user_jifen_img"
            android:layout_width="@dimen/_50px_in720p"
            android:layout_height="@dimen/_50px_in720p"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/_20px_in720p"
            android:layout_marginRight="@dimen/_20px_in720p"
            android:background="@drawable/my_self_jifen" />

        <TextView
            android:id="@+id/user_jifen_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/user_jifen_img"
            android:gravity="center_vertical"
            android:text="@string/user_jifen"
            android:textSize="@dimen/_32px_in720p" />

        <ImageView
            android:layout_width="@dimen/_20px_in720p"
            android:layout_height="@dimen/_30px_in720p"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/_20px_in720p"
            android:background="@drawable/back_xym" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/_20px_in720p"
        android:background="@color/gxian" />

    <RelativeLayout
        android:id="@+id/user_shoucang_lay"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_100px_in720p"
        android:background="@color/white" >

        <TextView
            android:id="@+id/user_shoucang_img"
            android:layout_width="@dimen/_50px_in720p"
            android:layout_height="@dimen/_50px_in720p"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/_20px_in720p"
            android:layout_marginRight="@dimen/_20px_in720p"
            android:background="@drawable/my_self_collect" />

        <TextView
            android:id="@+id/user_shoucang_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/user_shoucang_img"
            android:gravity="center_vertical"
            android:text="@string/user_shoucang"
            android:textSize="@dimen/_32px_in720p" />

        <ImageView
            android:layout_width="@dimen/_20px_in720p"
            android:layout_height="@dimen/_30px_in720p"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/_20px_in720p"
            android:background="@drawable/back_xym" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginLeft="@dimen/_20px_in720p"
        android:layout_marginRight="@dimen/_20px_in720p"
        android:background="@color/gxian" />

    <RelativeLayout
        android:id="@+id/user_dingdan_lay"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_100px_in720p"
        android:background="@color/white" >

        <TextView
            android:id="@+id/user_dingdan_img"
            android:layout_width="@dimen/_50px_in720p"
            android:layout_height="@dimen/_50px_in720p"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/_20px_in720p"
            android:layout_marginRight="@dimen/_20px_in720p"
            android:background="@drawable/my_self_address" />

        <TextView
            android:id="@+id/user_dingdan_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/user_dingdan_img"
            android:gravity="center_vertical"
            android:text="@string/user_dingdan_shops"
            android:textSize="@dimen/_32px_in720p" />

        <ImageView
            android:layout_width="@dimen/_20px_in720p"
            android:layout_height="@dimen/_30px_in720p"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/_20px_in720p"
            android:background="@drawable/back_xym" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginLeft="@dimen/_20px_in720p"
        android:layout_marginRight="@dimen/_20px_in720p"
        android:background="@color/gxian" />

    <RelativeLayout
        android:id="@+id/user_order_shops"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_100px_in720p"
        android:background="@color/white" >

        <TextView
            android:id="@+id/user_dingdan"
            android:layout_width="@dimen/_50px_in720p"
            android:layout_height="@dimen/_50px_in720p"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/_20px_in720p"
            android:layout_marginRight="@dimen/_20px_in720p"
            android:background="@drawable/my_self_address" />

        <TextView
            android:id="@+id/user_dingdan_foods"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/user_dingdan"
            android:gravity="center_vertical"
            android:text="@string/user_dingdan_foods"
            android:textSize="@dimen/_32px_in720p" />

        <ImageView
            android:layout_width="@dimen/_20px_in720p"
            android:layout_height="@dimen/_30px_in720p"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/_20px_in720p"
            android:background="@drawable/back_xym" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginLeft="@dimen/_20px_in720p"
        android:layout_marginRight="@dimen/_20px_in720p"
        android:background="@color/gxian" />

    <RelativeLayout
        android:id="@+id/user_address_lay"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_100px_in720p"
        android:background="@color/white" >

        <TextView
            android:id="@+id/user_address_img"
            android:layout_width="@dimen/_50px_in720p"
            android:layout_height="@dimen/_50px_in720p"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/_20px_in720p"
            android:layout_marginRight="@dimen/_20px_in720p"
            android:background="@drawable/my_self_order" />

        <TextView
            android:id="@+id/user_address_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/user_address_img"
            android:gravity="center_vertical"
            android:text="@string/user_address"
            android:textSize="@dimen/_32px_in720p" />

        <ImageView
            android:layout_width="@dimen/_20px_in720p"
            android:layout_height="@dimen/_30px_in720p"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/_20px_in720p"
            android:background="@drawable/back_xym" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/_20px_in720p"
        android:background="@color/gxian" />

    <RelativeLayout
        android:id="@+id/user_baoliao_lay"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_100px_in720p"
        android:background="@color/white" >

        <TextView
            android:id="@+id/user_baoliao_img"
            android:layout_width="@dimen/_50px_in720p"
            android:layout_height="@dimen/_50px_in720p"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/_20px_in720p"
            android:layout_marginRight="@dimen/_20px_in720p"
            android:background="@drawable/my_self_baoliao" />

        <TextView
            android:id="@+id/user_baoliao_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/user_baoliao_img"
            android:gravity="center_vertical"
            android:text="@string/user_baoliao"
            android:textSize="@dimen/_32px_in720p" />

        <ImageView
            android:layout_width="@dimen/_20px_in720p"
            android:layout_height="@dimen/_30px_in720p"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/_20px_in720p"
            android:background="@drawable/back_xym" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/_20px_in720p"
        android:background="@color/gxian" />

    <RelativeLayout
        android:id="@+id/user_set_lay"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_100px_in720p"
        android:background="@color/white" >

        <TextView
            android:id="@+id/user_set_img"
            android:layout_width="@dimen/_50px_in720p"
            android:layout_height="@dimen/_50px_in720p"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/_20px_in720p"
            android:layout_marginRight="@dimen/_20px_in720p"
            android:background="@drawable/my_self_set" />

        <TextView
            android:id="@+id/user_set_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/user_set_img"
            android:gravity="center_vertical"
            android:text="@string/user_set"
            android:textSize="@dimen/_32px_in720p" />

        <ImageView
            android:layout_width="@dimen/_20px_in720p"
            android:layout_height="@dimen/_30px_in720p"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/_20px_in720p"
            android:background="@drawable/back_xym" />
    </RelativeLayout>

</LinearLayout>